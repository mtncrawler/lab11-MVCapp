@model List<lab11_mvcApp.Models.TimePerson>;

<head>
    <link rel="stylesheet" href="~/StyleSheet.css" />
</head>
<body>
    <table>
        <tr>
            <th>Year</th>
            <th>Name</th>
            <th>Country</th>
            <th>Title</th>
            <th>Birth</th>
            <th>Death</th>
            <th>Category</th>
            <th>Context</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.Year</td>
            <td>@item.Name</td>
            <td>@item.Country</td>
            <td>@item.Title</td>
            <td>@item.Birth_Year</td>
            <td>@item.DeathYear</td>
            <td>@item.Category</td>
            <td>@item.Context</td>
        </tr>
        }
    </table>
    


</body>


